# DBOS Configuration (Required)
DBOS_SYSTEM_DATABASE_URL=postgresql://pipeline:pwd@localhost:5434/pipeline
DBOS_QUEUE_NAME=default
DBOS_QUEUE_CONCURRENCY=4

# Pipeline Standalone Configuration
# All-in-one service with embedded simple-content
STANDALONE_HTTP_ADDR=:8080
STORAGE_DIR=./dev-data

# Pipeline Worker Configuration
# Connects to separate simple-content API server
WORKER_HTTP_ADDR=:8081
CONTENT_API_URL=http://localhost:4000
